# AndeChain Infrastructure
ANDECHAIN_RPC_URL=http://localhost:8545
ANDECHAIN_EXPLORER_URL=http://localhost:4000

# Token Factory Configuration
TOKEN_FACTORY_CREATION_FEE=10000000000000000  # 0.01 ANDE
