# ANDE Token Duality Configuration - Testnet
# ==========================================

# Core ANDE Settings for Testnet
ANDE_PRECOMPILE_ENABLED=true
ANDE_TOKEN_ADDRESS=0x00000000000000000000000000000000000000FD
ANDE_NETWORK_ID=1234
ANDE_CHAIN_ID=1234

# Build Configuration for Testnet
EV_RETH_BUILD_TARGET=release
EV_RETH_FEATURES=ande-integration,mev-detection,parallel-execution

# Logging Configuration for Testnet
RUST_LOG=info,ev_reth=debug,ande_precompile=trace,mev=debug,parallel=info

# Testnet Settings
ANDE_DEBUG_MODE=false
ANDE_TEST_MODE=false
ANDE_PRODUCTION_MODE=true

# Performance Settings for Testnet
ANDE_MEMORY_LIMIT=4G
ANDE_CPU_LIMIT=4
ANDE_PARALLEL_THREADS=8

# MEV System Configuration
MEV_DETECTOR_ENABLED=true
MEV_AUCTION_ENABLED=true
MEV_DISTRIBUTOR_ENABLED=true
MEV_BUFFER_SIZE=1000
MEV_AUCTION_INTERVAL=30s

# Parallel Execution Configuration
PARALLEL_EXECUTION_ENABLED=true
PARALLEL_MAX_THREADS=16
PARALLEL_OPTIMISTIC_MVCC=true
PARALLEL_CONFLICT_RESOLUTION=true

# Monitoring for Testnet
ANDE_METRICS_ENABLED=true
ANDE_HEALTH_CHECK_INTERVAL=15s
ANDE_PROMETHEUS_PORT=9002
ANDE_GRAFANA_ENABLED=true

# Security Settings for Testnet
ANDE_SECURITY_MODE=true
ANDE_VALIDATION_STRICT=true
ANDE_RATE_LIMITING=true

# Testnet Network Configuration
ANDE_BOOTNODES=[]
ANDE_DISCOVERY_ENABLED=true
ANDE_P2P_PORT=30303